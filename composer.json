        }
    ],
    "require": {
        "php": ">=5.2.4",
        "vlucas/phpdotenv": "^2.0",
        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/psr7": "1.3.0",
        "aws/aws-sdk-php": "2.*",
        "phpseclib/phpseclib": "0.3.*",
        "iapps/common": "0.1.*",
        "moontoast/math": "^1.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.1.*",
        "phpunit/phpunit": "4.7.*",
        "raveren/kint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Iapps\\": "application/class/Iapps/",
            "Iafb\\": "application/class/Iafb/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php composer.phar dumpautoload -o"
        ],
        "post-update-cmd": [
            "php composer.phar dumpautoload -o"
        ]
    }
}
